{
    "name": "adr1enbe4udou1n/laravel-boilerplate",
    "description": "Laravel 5.6 Boilerplate based on Bootstrap 4 and Vue CoreUI for Backend.",
    "keywords": ["framework", "laravel", "boilerplate", "bootstrap 4 beta", "vue", "coreui"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-gd": "*",
        "arcanedev/no-captcha": "^6.0",
        "artesaos/seotools": "^0.11.1",
        "cviebrock/eloquent-sluggable": "^4.5",
        "davejamesmiller/laravel-breadcrumbs": "^5.0",
        "dimsav/laravel-translatable": "^9.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "hieu-le/active": "^3.5",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "laravel/framework": "5.6.*",
        "laravel/scout": "^4.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.6",
        "maatwebsite/excel": "^2.1",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.22",
        "mcamara/laravel-localization": "^1.3",
        "plank/laravel-mediable": "^2.5",
        "roumen/sitemap": "^2.8",
        "stevebauman/purify": "^2.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
        "tightenco/ziggy": "^0.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
