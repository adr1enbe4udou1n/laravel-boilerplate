{
    "name": "adr1enbe4udou1n/laravel-boilerplate",
    "description": "Laravel 5.7 Boilerplate based on Bootstrap 4 and Tabler for Backend.",
    "keywords": ["framework", "laravel", "boilerplate", "bootstrap 4 beta", "vue", "tabler"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "ext-gd": "*",
        "arcanedev/log-viewer": "^4.5",
        "arcanedev/no-captcha": "^7.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.1",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "hieu-le/active": "^3.5",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "laravel/framework": "5.7.*",
        "laravel/scout": "^5.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.7",
        "laravelium/sitemap": "^3.0",
        "league/csv": "^9.1",
        "maatwebsite/excel": "^3.0",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.22",
        "mcamara/laravel-localization": "^1.3",
        "spatie/laravel-medialibrary": "^7.1",
        "spatie/laravel-tags": "^2.0",
        "spatie/laravel-translatable": "^2.2",
        "stevebauman/purify": "^2.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
        "tightenco/ziggy": "^0.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.2",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
