{
    "name": "adr1enbe4udou1n/laravel-boilerplate",
    "description": "Laravel 5.5 Boilerplate based on Bootstrap 4 (beta) and 100% Vue.js backend.",
    "keywords": ["framework", "laravel", "boilerplate", "bootstrap 4 beta", "vue", "coreui"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1",
        "ext-gd": "*",
        "anhskohbo/no-captcha": "^2.4",
        "artesaos/seotools": "^0.10.0",
        "cviebrock/eloquent-sluggable": "^4.3",
        "davejamesmiller/laravel-breadcrumbs": "^4.0",
        "dimsav/laravel-translatable": "^8.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.2",
        "hieu-le/active": "^3.3",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "maatwebsite/excel": "^2.1",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.16",
        "mcamara/laravel-localization": "^1.2",
        "mews/purifier": "^2.0",
        "plank/laravel-mediable": "^2.5",
        "roumen/sitemap": "^2.6",
        "tightenco/ziggy": "^0.4.0",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "~2.0",
        "mockery/mockery": "1.0.0-alpha1",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
